class A {
    static function main() {
        var l = 0;
        
        function f(a, b) {
            if (a == 10) {
                throw l;
            }
            else {
                l = a + b;
            }
        
            return l;
        }
        
        function yote() {
            try {
                f(10, 23);
            }
            catch(e) {
                return e;
            }
            return l;
        }

        return yote()
    }
}