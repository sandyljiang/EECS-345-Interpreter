class A {
    static function main() {
        function factorial(n) {
            if (n == 0) {
                return 1;
            }
            else if (n == true) {
                throw false;
            }
            else {
                return n * factorial(n-1);
            }
        }
        
        var y = 5;
        
        function yote() {
            try {
                return factorial(true);
            }
            catch(yeet) {
                return yeet;
            }
        }

        return yote()
    }
}