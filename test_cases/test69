class A {
    static function main() {
        
        function f(x) {
          function g(x) {
            var b;
            b = x;
            return 0;
          }
        
          function h(x) {
            b = x;
            return 1;
          }
        
          return g(x) + h(x);
        }
        
        function yote() {
          return f(10);
        }

        return yote()
    }
}