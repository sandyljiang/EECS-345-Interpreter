class A {
    static function main() {
        
        var x = 0;
        var y = 0;
        
        function setx(a) {
          x = a;
        }
        
        function sety(b) {
          y = b;
        }
        
        function yote() {
          setx(5);
          sety(7);
          return x * y;
        }

        return yote()
    }
}