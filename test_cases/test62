class A {
    static function main() {
        
        var count = 0;
        
        function f(a,b) {
          count = count + 1;
          a = a + b;
          return a;
        }
        
        function yote() {
          f(1, 2);
          f(3, 4);
          return count;
        }

        return yote()
    }
}